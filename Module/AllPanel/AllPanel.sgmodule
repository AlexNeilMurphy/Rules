#!name=PciecnPanels
#!desc=包括重载配置、策略组面板、网络详情、流媒体解锁状态🥳

[Panel]
#Surge Pro标题,可显示启动时间,点击刷新为重载配置
Surge_Panel = script-name=Surge_Panel,update-interval=1

#!name=面板-汽油价格
QiYou = script-name=QiYou,update-interval=43200

#流量统计
TrafficStatistics = script-name=TrafficStatistics,update-interval=1

#网络详情
IP-Check = script-name=IP-Check, title="节点详情", content="请刷新", style=info, update-interval=1

#策略组面板 可重复配置 注意修改相应字段
groupPanel= script-name=groupPanel,update-interval=5

#流媒体解锁状态
stream-all = script-name=stream-all, title="流媒体解锁检测", content="请刷新面板", update-interval=3600

[Script]

#附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
Surge_Panel = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/Pciecn/Rules/main/Surge/Panels/Surge_Panel.js ,argument=icon=crown.fill&color=#f6c970

QiYou = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/cc63/Surge/main/Module/Panel/QiYou/Moore/QiYou.js,argument={{{省份拼音}}}

#流量统计 点击以切换网络界面
TrafficStatistics = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js ,argument=icon=arrow.up.arrow.down.circle&color=#5d84f8

#網路詳情 标题显示为根节点名
#应当修改的字段 group 代理策略组名称
IP-Check = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/Pciecn/Rules/main/Module/IP_Check/IP-Check.js,argument=icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=🚀 PROXY


#策略组面板 可重复配置 注意修改相应字段
#必须更改的字段：group 填写需要显示的策略组名称
groupPanel = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/groupPanel.js ,argument=icon=network&color=#86abee&group=🚀 PROXY

#流媒体解锁状态
stream-all = type=generic, timeout=15, script-path=https://raw.githubusercontent.com/LucaLin233/Luca_Conf/main/Surge/JS/stream-all.js

# > Flightradar24
Flightradar24 = type=http-response,pattern=^https:\/\/mobile\.flightradar24\.com\/mobile\/(user-session|subscribe),requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Yu9191/Rewrite/main/Flightradar24.js

[MITM]
hostname = mobile.flightradar24.com
